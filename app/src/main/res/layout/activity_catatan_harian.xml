            android:layout_width="match_parent" style="@style/groupStylegray_300cornerRadius">
            <LinearLayout android:id="@+id/linearRowcategory"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_30pxv"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCategory"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_category_black_901"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_category_black_901"/>
                <ImageView android:id="@+id/imageCalendar"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_20pxh"
