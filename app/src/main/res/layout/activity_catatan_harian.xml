            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackvector"
                android:layout_height="@dimen/_230pxv" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameStackgroup106"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_226pxv"
                    android:layout_marginTop="@dimen/_4pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageGroup106"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_226pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group106"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group106"/>
                    <FrameLayout
                        android:id="@+id/frameStackjonathanborba"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_95pxv"
                        android:layout_marginBottom="@dimen/_59pxv"
                        android:layout_marginEnd="@dimen/_152pxh"
