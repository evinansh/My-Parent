<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="editProfilVM" type="com.evinanshsapplication.app.modules.editprofil.data.viewmodel.EditProfilVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearEditProfil"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnrectangle4207"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackvector"
                android:layout_height="@dimen/_226pxv" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameStackrectangle4207"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_226pxv" android:layout_width="match_parent">
                    <View android:background="@color/gray_300"
                        android:id="@+id/viewRectangle4207"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_182pxv"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_375pxh"/>
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_187_5"
                        android:id="@+id/viewEllipseSeven"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_109pxv"
                        android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_375pxh"/>
                </FrameLayout>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowvector"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStyledeep_purple_300">
                        <ImageView android:id="@+id/imageVector"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginBottom="@dimen/_23pxv"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_width="@dimen/_9pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_900"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTitle"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_marginEnd="@dimen/_134pxh"
                            android:layout_marginStart="@dimen/_106pxh"
                            android:layout_marginTop="@dimen/_19pxv"
                            android:layout_weight="0.92"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{editProfilVM.editProfilModel.txtTitle}"
                            style="@style/txtPlusJakartaSansromanbold20" tools:text="@string/lbl_edit_profile"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <FrameLayout android:id="@+id/frameStackjonathanborba"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_95pxv"
                    android:layout_marginBottom="@dimen/_48pxv"
                    android:layout_marginEnd="@dimen/_148pxh"
                    android:layout_marginStart="@dimen/_148pxh"
                    android:layout_marginTop="@dimen/_48pxv" android:layout_width="@dimen/_69pxh">
                    <LinearLayout
                        android:id="@+id/linearColumnjonathanborba"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_4pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                        <ImageView android:gravity="center_horizontal"
                            android:id="@+id/imageJonathanborba"
                            android:layout_height="@dimen/_48pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginEnd="@dimen/_5pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_jonathanborba}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_jonathanborba"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtDuration"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{editProfilVM.editProfilModel.txtDuration}"
                        style="@style/txtPlusJakartaSansromanbold8" tools:text="@string/lbl_2_month_old"/>
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtTitleOne"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{editProfilVM.editProfilModel.txtTitleOne}"
                        style="@style/txtPlusJakartaSansromanbold20" tools:text="@string/lbl_syakila"/>
                    <View
                        android:background="@drawable/rectangle_bg_black_900_4c_radius_7_5"
                        android:id="@+id/viewRectangle4206"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_62pxh"/>
                </FrameLayout>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtUsername"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfilVM.editProfilModel.txtUsername}"
                style="@style/txtPoppinsmedium14" tools:text="@string/lbl_username"/>
            <EditText
                android:background="@drawable/rectangle_border_gray_500_radius_8"
                android:ellipsize="end"
                android:id="@+id/etRectangleFortySix"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@={editProfilVM.editProfilModel.etRectangleFortySixValue}"
                style="@style/etRoundedOutline_1" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEmailId"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfilVM.editProfilModel.txtEmailId}"
                style="@style/txtPoppinsmedium14" tools:text="@string/lbl_email_i_d"/>
            <EditText
                android:background="@drawable/rectangle_border_gray_500_radius_8"
                android:ellipsize="end"
                android:id="@+id/etRectangleFortySeven"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@={editProfilVM.editProfilModel.etRectangleFortySevenValue}"
                style="@style/etRoundedOutline_1" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPhoneNumber"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfilVM.editProfilModel.txtPhoneNumber}"
                style="@style/txtPoppinsmedium14" tools:text="@string/lbl_phone_number"/>
            <EditText
                android:background="@drawable/rectangle_border_gray_500_radius_8"
                android:ellipsize="end"
                android:id="@+id/etRectangleFortyEight"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@={editProfilVM.editProfilModel.etRectangleFortyEightValue}"
                style="@style/etRoundedOutline_1" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPassword"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfilVM.editProfilModel.txtPassword}"
                style="@style/txtPoppinsmedium14" tools:text="@string/lbl_password"/>
            <EditText
                android:background="@drawable/rectangle_border_gray_500_radius_8"
                android:ellipsize="end"
                android:id="@+id/etRectangleFortyNine"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@={editProfilVM.editProfilModel.etRectangleFortyNineValue}"
                style="@style/etRoundedOutline_1" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnUpdate"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_53pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_12pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_12pxv"
                android:singleLine="true"
                android:text="@string/lbl_update"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" tools:text="@string/lbl_update"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylegray_300cornerRadius">
            <LinearLayout android:id="@+id/linearRowcategory"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_30pxv"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCategory"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_category_black_901_20x20"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_category_black_901_20x20"/>
                <ImageView android:id="@+id/imageFolder"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_folder"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_folder"/>
                <ImageView android:id="@+id/imageUnion"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_union"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginEnd="@dimen/_60pxh"
                    android:layout_marginStart="@dimen/_61pxh"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_deep_purple_a200"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_deep_purple_a200"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
